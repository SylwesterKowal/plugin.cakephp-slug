{
  "name": "SylwesterKowal/plugin.cakephp-slug",
  "description": "Slug plugin for CakePHP",
  "type": "cakephp-plugin",
  "repositories": [
    {
      "url": "https://github.com/SylwesterKowal/plugin.cakephp-slug",
      "type": "git"
    }
  ],
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Slug\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slug\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
