{
    "name": "sylwesterkowal/plugin.cakephp-slug",
    "description": "CakePHP Plugin for Slugging",
    "type": "cakephp-plugin",
    "authors": [
        {
            "name": "SK",
            "email": "sylwester.kowal@21w.pl"
        }
    ],
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.4.16",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Slug\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Slug\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    },
    "minimum-stability": "dev",
    "require": {}
}