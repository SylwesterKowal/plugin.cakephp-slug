{
  "name": "sylwesterkowal/plugin.cakephp-slug",
  "description": "CakePHP Plugin for Slugging",
  "keywords": ["cakephp3","slug","plugin"],
  "homepage": "https://github.com/SylwesterKowal/plugin.cakephp-slug",
  "type": "cakephp-plugin",
  "license": "MIT",
  "authors": [
    {
      "name": "Sylwester Kowal",
      "email": "sylwester.kowal@21w.pl"
    }
  ],
  "type": "cakephp-plugin",
  "require": {
    "php": ">=5.4.16",
    "cakephp/cakephp": "~3.0"
  },
  "require-dev": {
    "phpunit/phpunit": "*"
  },
  "autoload": {
    "psr-4": {
      "Slug\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Slug\\Test\\": "tests",
      "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
    }
  },
  "minimum-stability": "stable"
}